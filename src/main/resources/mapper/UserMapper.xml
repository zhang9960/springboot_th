<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.z.springboot_thymeleaf.mapper.UserMapper">

    <select id="select" resultType="com.z.springboot_thymeleaf.entity.User">
        select * from st_login where username=#{username} and password=#{password}
    </select>
</mapper>
